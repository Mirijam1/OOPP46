<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.shape.Line?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="240.0" prefWidth="500.0" style="-fx-background-color: #EEFFF2;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.ErrorController">
   <children>
      <Label fx:id="error" layoutX="43.0" layoutY="25.0" text="ERROR" textFill="#868484">
         <font>
            <Font name="Microsoft YaHei Light" size="36.0" />
         </font>
      </Label>
      <Label fx:id="content1" layoutX="148.0" layoutY="107.0" prefHeight="26.0" prefWidth="224.0" text="Something went wrong. " textAlignment="CENTER" textFill="#7b7b7b">
         <font>
            <Font name="Microsoft JhengHei Light" size="18.0" />
         </font>
      </Label>
      <Line fx:id="line" endX="350.0" layoutX="109.0" layoutY="86.0" startX="-100.0" stroke="#9e9999" />
      <Label fx:id="content2" layoutX="157.0" layoutY="133.0" text="Please try again later." textFill="#7b7b7b">
         <font>
            <Font name="Microsoft JhengHei Light" size="18.0" />
         </font>
      </Label>
      <Button fx:id="goback" layoutX="203.0" layoutY="177.0" mnemonicParsing="false" onAction="#exit" stylesheets="@../css/button.css" text="GO BACK" textFill="#7b7b7b" />
      <ImageView fx:id="image" fitHeight="57.0" fitWidth="53.0" layoutX="407.0" layoutY="24.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../img/error.jpg" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
