<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.shape.Line?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="240.0" prefWidth="500.0" style="-fx-background-color: #EEFFF2;" stylesheets="@../css/button.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="nl.tudelft.gogreen.gui.controllers.helpers.ConfirmationController">
   <children>
      <Label fx:id="content" layoutX="61.0" layoutY="106.0" text="Are you sure you want to add these?" textFill="#7b7b7b">
         <font>
            <Font name="Microsoft YaHei Light" size="20.0" />
         </font>
      </Label>
      <Label fx:id="confirmation" layoutX="22.0" layoutY="24.0" text="Confirmation" textFill="#7b7b7b">
         <font>
            <Font name="Microsoft YaHei Light" size="36.0" />
         </font>
      </Label>
      <Button fx:id="confirm" layoutX="107.0" layoutY="166.0" mnemonicParsing="false" onAction="#confirmButton" prefHeight="30.0" prefWidth="100.0" stylesheets="@../css/button.css" text="CONFIRM" textFill="#7b7b7b">
         <font>
            <Font name="Microsoft YaHei Light" size="15.0" />
         </font>
      </Button>
      <Button fx:id="cancel" layoutX="289.0" layoutY="166.0" mnemonicParsing="false" onAction="#exit" prefHeight="30.0" prefWidth="100.0" stylesheets="@../css/button.css" text="CANCEL" textFill="#7b7b7b">
         <font>
            <Font name="Microsoft JhengHei Light" size="15.0" />
         </font>
      </Button>
      <Line endX="350.0" layoutX="109.0" layoutY="86.0" startX="-100.0" stroke="#9e9999" />
      <ImageView fx:id="image" fitHeight="57.0" fitWidth="53.0" layoutX="407.0" layoutY="24.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../img/confirmation.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
